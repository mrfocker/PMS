<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="cn.pms.ssm.mapper.ShowTeacherInfoMapper">

<!--填充教师信息到页面-->
<select id="getTeacherInfo" parameterType="cn.pms.ssm.po.Teacher" resultType="cn.pms.ssm.po.Teacher">
		SELECT
		 teacher.Teacher_id,
		 teacher.Teacher_name
		FROM 
		  teacher
		WHERE 
		  Teacher_id = #{Teacher_id}
</select>
</mapper>